<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="minimine.properties"/>
  <sources>

    <!-- Trifolium/pratense/about_this_collection -->
    <source name="Trifolium-pratense-description" type="lis-description" dump="true" >
      <property name="dataSetName" value="Trifolium/pratense/about_this_collection" />
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Trifolium/pratense/about_this_collection/" />
      <property name="dataSetDescription" value="Trifolium pratense organism and strain information." />
      <property name="src.data.dir" location="/data/v2/Trifolium/pratense/about_this_collection" />
    </source>
    <!-- Trifolium/pratense/genomes/MilvusB.gnm2.gNmT -->
    <source name="MilvusB.gnm2.gNmT" type="lis-fasta" dump="true">
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Trifolium/pratense/genomes/MilvusB.gnm2.gNmT/" />
      <property name="lis-fasta.className" value="org.intermine.model.bio.Chromosome" />
      <property name="lis-fasta.includes" value="README.MilvusB.gnm2.gNmT.yml,tripr.MilvusB.gnm2.gNmT.genome_main.fna.gz" />
      <property name="src.data.dir" location="/data/v2/Trifolium/pratense/genomes/MilvusB.gnm2.gNmT"/>
    </source>
    <!-- Trifolium/pratense/annotations/MilvusB.gnm2.ann1.DFgp -->
    <source name="MilvusB.gnm2.ann1.DFgp" type="lis-annotation" dump="true">
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Trifolium/pratense/annotations/MilvusB.gnm2.ann1.DFgp/" />
      <property name="src.data.dir" location="/data/v2/Trifolium/pratense/annotations/MilvusB.gnm2.ann1.DFgp"/>
    </source>
    
    <!-- Phaseolus/vulgaris/about_this_collection -->
    <source name="Phaseolus-vulgaris-description" type="lis-description" dump="true" >
      <property name="dataSetName" value="Phaseolus/vulgaris/about_this_collection" />
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Phaseolus/vulgaris/about_this_collection/" />
      <property name="dataSetDescription" value="Phaseolus vulgaris organism and strain information." />
      <property name="src.data.dir" location="/data/v2/Phaseolus/vulgaris/about_this_collection" />
    </source>
    <!-- Phaseolus/vulgaris/genomes/G19833.gnm2.fC0g -->
    <source name="G19833.gnm2.fC0g" type="lis-fasta" dump="true">
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Phaseolus/vulgaris/genomes/G19833.gnm2.fC0g/" />
      <property name="lis-fasta.className" value="org.intermine.model.bio.Chromosome" />
      <property name="lis-fasta.includes" value="README.G19833.gnm2.fC0g.yml,phavu.G19833.gnm2.fC0g.genome_main.fna.gz" />
      <property name="src.data.dir" location="/data/v2/Phaseolus/vulgaris/genomes/G19833.gnm2.fC0g"/>
    </source>
    <!-- Phaseolus/vulgaris/annotations/G19833.gnm2.ann1.PB8d -->
    <source name="G19833.gnm2.ann1.PB8d" type="lis-annotation" dump="true">
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Phaseolus/vulgaris/annotations/G19833.gnm2.ann1.PB8d/" />
      <property name="src.data.dir" location="/data/v2/Phaseolus/vulgaris/annotations/G19833.gnm2.ann1.PB8d"/>
    </source>




    
    <!-- ############### -->
    <!-- ONTOLOGIES ETC. -->
    <!-- ############### -->

    <!-- LIS gene families -->
    <source name="legume.genefam.fam1.M65K" type="lis-genefamily" dump="true">
      <property name="dataSetName" value="legume.genefam.fam1.M65K"/>
      <property name="dataSetUrl" value="https://data.legumeinfo.org/LEGUMES/Fabaceae/genefamilies/legume.genefam.fam1.M65K/" />
      <property name="dataSetDescription" value="LIS gene families"/>
      <property name="src.data.dir" location="/data/v2/LEGUMES/Fabaceae/genefamilies/legume.genefam.fam1.M65K"/>
    </source>

    <!-- Phytozome gene families -->
    <source name="phytozome_10_2.genefam.descriptors" type="lis-phytozome" dump="true">
      <property name="dataSetName" value="phytozome_10_2.genefam.descriptors"/>
      <property name="dataSetDescription" value="Phytozome gene family descriptions"/>
      <property name="src.data.dir" location="/data/phytozome"/>
    </source>

    <!-- LEGUMES/Fabaceae/genefamilies/legume.genefam.fam1.M65K/legume.genefam.fam1.M65K.trees_ML_rooted/ -->
    <source name="legume.genefam.fam1.M65K.trees_ML_rooted" type="lis-phylotree" dump="true">
      <property name="dataSetName" value="legume.genefam.fam1.M65K.trees_ML_rooted"/>
      <property name="dataSetUrl" value="https://legumeinfo.org/data/v2/LEGUMES/Fabaceae/genefamilies/legume.genefam.fam1.M65K/legume.genefam.fam1.M65K.trees_ML_rooted/"/>
      <property name="dataSetDescription" value="LIS gene family phylogenetic trees"/>
      <property name="src.data.dir" location="/data/v2/LEGUMES/Fabaceae/genefamilies/legume.genefam.fam1.M65K/legume.genefam.fam1.M65K.trees_ML_rooted"/>
    </source>
    
    <!-- INTERPRO -->
    <source name="interpro" type="interpro">
      <property name="data.source.name" value="InterPro"/>
      <property name="data.source.description" value="XML file listing each InterPro entry."/>
      <property name="data.source.url" value="ftp://ftp.ebi.ac.uk/pub/databases/interpro/"/>
      <property name="src.data.dir" location="/data/InterPro/"/>
    </source>
    <source name="interpro-go" type="interpro-go">
      <property name="data.source.name" value="InterPro2GO"/>
      <property name="data.source.description" value="Mappings of InterPro entries to Gene Ontology (GO) terms."/>
      <property name="data.source.url" value="ftp://ftp.ebi.ac.uk/pub/databases/interpro/"/>
      <property name="src.data.dir" location="/data/InterPro/ontology/"/>
    </source>

    <!-- gene ontology -->
    <source name="go" type="go">
      <property name="src.data.file" location="/data/gene-ontology/go-basic.obo"/>
    </source>

    <!-- sequence ontology -->
    <source name="so" type="so">
      <property name="src.data.file" location="/data/SO-Ontologies/so-simple.obo"/>
    </source>

    <!-- plant ontology -->
    <source name="plant-ontology" type="obo">
      <property name="src.data.file" location="/data/plant-ontology/po.obo"/>
      <property name="ontology.prefix" value="PO"/>
      <property name="obo.ontology.name" value="Plant Ontology"/>
      <property name="obo.ontology.url" value="http://planteome.org/"/>
      <property name="obo.ontology.licence" value="https://creativecommons.org/licenses/by/4.0/"/>
      <property name="obo.ontology.description" value="The Plant Ontology (PO) is a community resource consisting of standardized terms, definitions, and logical relations describing plant structures and development stages, augmented by a large database of annotations from genomic and phenomic studies."/>
    </source>

    <!-- other species from synteny -->
    <source name="aradu-description" type="lis-description" dump="true">
      <property name="dataSetName" value="Arachis/duranensis/about_this_collection"/>
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Arachis/duranensis/about_this_collection/" />
      <property name="dataSetDescription" value="Glycine max organism and strain information."/>
      <property name="src.data.dir" location="/data/v2/Arachis/duranensis/about_this_collection"/>
