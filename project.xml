      <property name="dataSetUrl" value="https://data.legumeinfo.org/Glycine/max/about_this_collection/" />
      <property name="dataSetDescription" value="Glycine max organism and strain information."/>
      <property name="src.data.dir" location="/data/v2/Glycine/max/about_this_collection"/>
    </source>
    <source name="lotja-description" type="lis-description" dump="true">
      <property name="dataSetName" value="Lotus/japonicus/about_this_collection"/>
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Lotus/japonicus/about_this_collection/" />
      <property name="dataSetDescription" value="Glycine max organism and strain information."/>
      <property name="src.data.dir" location="/data/v2/Lotus/japonicus/about_this_collection"/>
    </source>
    <source name="lupan-description" type="lis-description" dump="true">
      <property name="dataSetName" value="Lupinus/angustifolius/about_this_collection"/>
      <property name="dataSetUrl" value="https://data.legumeinfo.org/Lupinus/angustifolius/about_this_collection/" />
      <property name="dataSetDescription" value="Glycine max organism and strain information."/>
      <property name="src.data.dir" location="/data/v2/Lupinus/angustifolius/about_this_collection"/>
    </source>
    <source name="phavu-description" type="lis-description" dump="true">
      <property name="dataSetName" value="Phaseolus/vulgaris/about_this_collection"/>
